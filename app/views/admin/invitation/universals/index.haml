- stylesheet 'admin'
- unless @postings.empty?
  .grid_14.push_1.admin.canvas.invitation
    %table
      %tr
        %th Inviter
        %th Code
        %th Created
        %th State
        %th Expiration
      - @postings.each do |posting|
        %tr
          %td= posting.user.handle(current_site)
          %td= posting.code
          %td= posting.created_at.to_date.to_s(:long)
          %td= posting.state.titleize
          %td n/a
